<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
    <rules>
        <rule name="Redirect to index" stopProcessing="true">
            <match url="(.*)" />
            <conditions>
                <add input="{HTTP_HOST}" pattern="^zmejelov\.azurewebsites\.net$" />
            </conditions>
            <action type="Redirect" url="https://zmejelov.azurewebsites.net/index.php" redirectType="Permanent" />
        </rule>
    </rules>
</rewrite>

    </system.webServer>
</configuration>
